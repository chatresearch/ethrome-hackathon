# Your agent's wallet key (hex format, WITH 0x prefix)
# Generate with: node -e "console.log('0x' + require('crypto').randomBytes(32).toString('hex'))"
XMTP_WALLET_KEY=your_wallet_key_here

# Database encryption key (WITHOUT 0x prefix)
# Generate with: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))"
# IMPORTANT: Keep this secret and backed up!
XMTP_DB_ENCRYPTION_KEY=your_encryption_key_here

# XMTP network (dev or production)
XMTP_ENV=production
